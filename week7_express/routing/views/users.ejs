<html>
<head>
	<meta charset="utf-8">
	<title>EJS View</title>
</head>
<body>
	<h1>Here are all the users:</h2>
	<% for (var x in users) { %>
		<h3><a href="/users/<%= x %>">Nmae: <%= users[x].name %></a></h3>
		<h4>Email: <%= users[x].email %></h4>

		<hr/>

	<% } %>
	<a href="/users/new">Add a user</a>
</body>
</html>