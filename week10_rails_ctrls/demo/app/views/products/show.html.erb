<div class="container">
	<div class="row">
		<div class="col-md-6">
			<h1>Product <%= @product.id %></h1>
			<p><%= @product.name %></p>
			<p><%= @product.description %></p>
			<p><%= @product.created_at %></p>
		</div>
	</div>
	<div class="row">
		<div class="col-md-6 text-left">
			<a href="/products">
				<button class="btn btn-primary">Go Back</button>
			</a>
		</div>
	</div>
</div>